{
  "name": "bankool",
  "version": "1.0.0",
  "description": "An api to provide services to bankool clients",
  "main": "index.js",
  "license": "unlicense",
  "author": "Marcos Soares",
  "private": true,
  "scripts": {
    "start": "yarn clean && node -r dotenv/config dist/main",
    "start:container": "yarn clean && yarn up",
    "start:dev": "yarn up:database && ts-node-dev --respawn --transpile-only --clear -r dotenv/config --inspect=0.0.0.0:9229 src/main",
    "debug:container": "npm install -g nodemon@2.0.15 && nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/index.js",
    "up": "docker-compose up -d",
    "up:database": "docker-compose up -d database",
    "clean": "yarn rimraf dist",
    "build": "yarn clean && yarn tsc -p tsconfig-build.json",
    "build:watch": "yarn clean && yarn tsc -p tsconfig-build.json -w",
    "check-deps": "yarn yarn-check -s -u",
    "lint": "yarn eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "yarn jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:coverage": "yarn test --coverage --config ./jest.all.config.js",
    "test:all": "yarn test --config ./jest.all.config.js",
    "test:integration": "yarn test --config ./jest.integration.config.js",
    "test:coveralls": "yarn test:coverage && coveralls < coverage/lcov.info",
    "prepare": "husky install"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.0",
    "mongodb": "4.3.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@jest-mock/express": "1.4.5",
    "@shelf/jest-mongodb": "^2.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "17.0.12",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "jest-mock-extended": "^2.0.1",
    "lint-staged": "^12.3.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.5.5",
    "yarn-check": "^0.0.3"
  },
  "engines": {
    "node": "16.x"
  }
}
