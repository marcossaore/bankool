{
  "name": "bankool",
  "version": "1.0.0",
  "description": "An api to provide services to bankool clients",
  "main": "index.js",
  "author": "Marcos Soares",
  "scripts": {
    "clean": "yarn rimraf dist",
    "build": "yarn clean && yarn tsc -p tsconfig-build.json",
    "check-deps": "yarn yarn-check -s -u",
    "lint": "yarn eslint .",
    "lint:fix": "yarn lint --fix",
    "test": "yarn jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "yarn test --watch",
    "test:staged": "yarn test --findRelatedTests",
    "test:coverage": "yarn test --coverage",
    "test:integration": "yarn test --config ./jest.integration.config.js",
    "test:coveralls": "yarn test:coverage && coveralls < coverage/lcov.info",
    "prepare": "husky install"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cpf-cnpj-validator": "^1.0.3",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.0",
    "mongodb": "4.3.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@jest-mock/express": "1.4.5",
    "@shelf/jest-mongodb": "^2.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/module-alias": "^2.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "17.0.12",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^7.0.2",
    "jest": "^27.1.1",
    "jest-mock-extended": "^2.0.1",
    "lint-staged": "^12.3.1",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.5.5",
    "yarn-check": "^0.0.3"
  },
  "engines": {
    "node": "16.x"
  }
}
